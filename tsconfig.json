{
  "compilerOptions": {
    "typeRoots": [ "./node_modules/@types", "./node_modules/typescript/lib" ],
    "alwaysStrict": true,
    "emitDecoratorMetadata": false,
    "importHelpers": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "target": "es2017",
    "outDir": "compiled",
    "lib": [
      "es2015.promise",
      "dom",
      "es6",
      "scripthost",
      "es7",
      "es2017.object",
      "es2015.proxy"
    ],
    "baseUrl": ".",
    "paths": {
      "ava": ["node_modules/ava/types/generated.d.ts"],
      "tslib" : ["node_modules/tslib/tslib.d.ts"]
    }
  }
}